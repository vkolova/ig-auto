{
    "presets":[
        ["@babel/preset-env", {
            "modules": "commonjs"
        }],
        "@babel/preset-react"
    ],
    "plugins": [
        ["@babel/plugin-proposal-class-properties", {
            "loose": true
        }],
        "@babel/plugin-transform-runtime",
        ["@babel/plugin-proposal-private-property-in-object", { "loose": true }]
    ]
}